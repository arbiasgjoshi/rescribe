{
    "name": "rescribe",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "docs",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md,mdx}\""
    },
    "devDependencies": {
        "@rescribe/eslint-config": "workspace:*",
        "prettier": "^2.8.3",
        "prettier-plugin-tailwindcss": "^0.2.2",
        "turbo": "^1.7.0"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "packageManager": "pnpm@7.26.0"
}
